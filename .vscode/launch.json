    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Azure Functions",
            "type": "node",
            "request": "attach",
            "port": 5858
        },
        {
            "type": "node",
            "request": "launch",
            "name": "tools/shp2json/test_utf8_points",
            "program": "${workspaceFolder}/tools/shp2json/shp2json.js",
            "args": [
                "--encoding",
                "utf8",
                "-o",
                "${workspaceFolder}/tools/shp2json/test/out/points.geojson",
                "${workspaceFolder}/tools/shp2json/test/points.shp"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "tools/shp2json/test_utf8_polygons",
            "program": "${workspaceFolder}/tools/shp2json/shp2json.js",
            "args": [
                "--encoding",
                "utf8",
                "-o",
                "${workspaceFolder}/tools/shp2json/test/out/polygons.geojson",
                "${workspaceFolder}/tools/shp2json/test/polygons.shp"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "tools/shp2json/test_shiftjis_01100札幌市2017_5",
            "program": "${workspaceFolder}/tools/shp2json/shp2json.js",
            "args": [
                "--encoding",
                "shift_jis",
                "--convert-coordinates",
                "--epsg-from",
                "+proj=tmerc +lat_0=44 +lon_0=142.25 +k=0.9999 +x_0=0 +y_0=0 +ellps=GRS80 +towgs84=0,0,0,0,0,0,0 +units=m +no_defs",
                "--epsg-to",
                "+proj=longlat +datum=WGS84 +no_defs",
                "-o",
                "${workspaceFolder}/tools/shp2json/test/out/01100札幌市2017_5.geojson",
                "${workspaceFolder}/tools/shp2json/test/01100札幌市2017_5.shp"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "tools/shp2json/test_shiftjis_01634鹿追町2017_5",
            "program": "${workspaceFolder}/tools/shp2json/shp2json.js",
            "args": [
                "--encoding",
                "shift_jis",
                "--convert-coordinates",
                "--epsg-from",
                "+proj=tmerc +lat_0=44 +lon_0=144.25 +k=0.9999 +x_0=0 +y_0=0 +ellps=GRS80 +towgs84=0,0,0,0,0,0,0 +units=m +no_defs",
                "-o",
                "${workspaceFolder}/tools/shp2json/test/out/01634鹿追町2017_5.geojson",
                "${workspaceFolder}/tools/shp2json/test/01634鹿追町2017_5.shp"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "tools/json2mssql/test_points_geojson",
            "program": "${workspaceFolder}/tools/json2mssql/json2mssql.js",
            "args": [
                "-o",
                "${workspaceFolder}/tools/json2mssql/test/out/points.sql",
                "--table",
                "PointGeometries",
                "--geo-column",
                "PinGeometry",
                "${workspaceFolder}/tools/json2mssql/test/points.geojson"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "tools/json2mssql/test_polygons_geojson",
            "program": "${workspaceFolder}/tools/json2mssql/json2mssql.js",
            "args": [
                "-o",
                "${workspaceFolder}/tools/json2mssql/test/out/polygons.sql",
                "--srid",
                "100",
                "${workspaceFolder}/tools/json2mssql/test/points.geojson"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "tools/json2mssql/test_01100札幌市2017_5_geojson",
            "program": "${workspaceFolder}/tools/json2mssql/json2mssql.js",
            "args": [
                "-o",
                "${workspaceFolder}/tools/json2mssql/test/out/01100札幌市2017_5.sql",
                "--table",
                "GenkyoPolygons",
                "--geo-column",
                "PolGeometry",
                "--middleware",
                "${workspaceFolder}/tools/json2mssql/middlewares/GenkyoPolygons.js",
                "${workspaceFolder}/tools/json2mssql/test/01100札幌市2017_5.geojson"
            ]
        }
    ]
}